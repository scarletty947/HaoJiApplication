<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.haojiapplication">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReasultActivity"></activity>
        <activity
            android:name=".BookContentActivity"
            android:label="@string/mynote"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity android:name=".AddBookActivity" />
        <activity
            android:name=".BookListActivity"
            android:label="@string/booklist" />
        <activity android:name=".Picture" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app"
            android:windowSoftInputMode="stateHidden|adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>